<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alserdar89.onewaymessage">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/one_way_message"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/one_way_message"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".welcome_screen.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TheWholeFuckinMessage" />
        <activity
            android:name=".SendTheMessageActivity"
            android:windowSoftInputMode="stateVisible" />
        <activity android:name=".login.GoogleSignInActivity" />
        <activity android:name=".login.DetectCountry" />
        <activity android:name=".login.SingleSignIn" />
        <activity android:name=".TheHome" />
        <activity android:name=".Launcher">
        </activity>
    </application>

</manifest>